            "homepage" : "http://alanseiden.com"
        }, {
            "name" : "Adam Culp",
            "email" : "adamculp@uws.net",
            "homepage" : "http://geekyboy.com"
        }, {
            "name" : "Guido Faecke",
            "email" : "guido@faecke.us",
            "homepage" : "http://faecke.us"
        }
